package phase1java;

public class CallByValue {
	int val = 150;
	int operation(int val)
	{
		val=val*10/100;
		return val;
	}

public static void main(String[] args) {
	
	CallByValue d = new CallByValue();
	
	System.out.println("before:"+ d.val);
	int f= d.operation(100);
	System.out.println("after:"+ f);
}
}
